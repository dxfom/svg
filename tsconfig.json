{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "jsx": "react-jsx",
    "jsxImportSource": ".",
    "declaration": true,
    "strict": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "newLine": "LF",
    "baseUrl": ".",
    "noEmit": true
  },
  "include": ["src/**/*"]
}
